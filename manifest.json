{
    "manifest_version": 3,
    "name": "LeetCode Torture",
    "version": "1.3",
    "description": "It will haunt you until you solve the leetcode potd",
    "permissions": ["declarativeNetRequest", "storage", "webNavigation"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [],
    "host_permissions": ["<all_urls>"]
  }
  